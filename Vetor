
public class FunçaoString{
    public static void main(String[]args){
        
        String s = "batata maça limao orange ";
        
        String[] vect = s.split(" ");
        
        System.out.println(vect[0]);
        System.out.println(vect[1]);
        System.out.println(vect[2]);
        System.out.println(vect[3]);

        



    }
}
